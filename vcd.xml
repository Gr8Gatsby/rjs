<?xml version="1.0" encoding="utf-8"?>
<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.2">
  <CommandSet xml:lang="en-us" Name="RJS">
    <CommandPrefix>RJSo</CommandPrefix>
    <Example>RJS show camera api</Example>
    <Command Name="API">
      <Example>show {message} using rjs</Example>
      <ListenFor RequireAppName="BeforeOrAfterPhrase">Please show {apiName}</ListenFor>
      <Feedback>showing you {apiName} with RJS</Feedback>
      <Navigate Target="/showAPI.htm"/>
    </Command>
    <PhraseTopic Label="apiName" Scenario="Dictation"></PhraseTopic>
  </CommandSet>
</VoiceCommands>